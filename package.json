{
  "name": "beta-doc-backend",
  "type": "module",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "swagger": "node src/index.js --swagger",
    "test:auth": "node tests/test_auth.js",
    "test:consultation-types": "node tests/add_consultation_types.js",
    "test:flow": "node tests/test_consultation_flow.js",
    "test:users": "node tests/test_users.js",
    "test:update-availability": "node tests/update_doctor_availability.js",
    "test:slot-booking": "node tests/test_consultation_slot_booking.js",
    "test:listings": "node tests/test_consultation_listings.js",
    "update:schema": "node tests/update_schema.js",
    "update:consultation-fields": "node -e \"console.log('Running consultation fields schema update...'); require('fs').readFileSync('consultation_fields_update.sql', 'utf8').split(';').forEach(query => query.trim() && console.log(query));\""
  },
  "keywords": [
    "healthcare",
    "api",
    "express",
    "supabase",
    "jwt",
    "authentication",
    "patients",
    "doctors",
    "consultations"
